md-content.timesheet-navigation-content

  md-list
    md-list-item.navigation-item(tabindex="10" ng-click='gotoTimesheet(timesheet)' ng-repeat='timesheet in timesheets track by $index',
                  ng-class='{"is-active": isTimesheetActive(timesheet)}')

      div(layout='column')
        h4() {{getTimesheetDate(timesheet)}}
        h5(ng-if='timesheet.status === "Created"')
         em {{getTimesheetTimeframe(timesheet)}}

      md-icon.timesheet-approved-tick(ng-show='timesheet.status === "Created"', md-font-set="material-icons") check_box_outline_blank
      md-icon.timesheet-approved-tick(ng-show='timesheet.status !== "Created"', md-font-set="material-icons") check_box
